        <linkType>url</linkType>
        <masterLabel>Refresh Opportunities</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <url>{!URLFOR(&apos;/apex/npsp__RD_AddDonationsBTN?id=&apos; + Recurring_Donation__c.Id)}</url>
    </webLinks>
</CustomObject>
